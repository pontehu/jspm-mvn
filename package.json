  "version": "0.6.3",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/zenorbi/jspm-mvn"
  },
  "bugs": {
    "url": "https://github.com/zenorbi/jspm-mvn/issues"
  },
  "devDependencies": {
    "@types/bluebird": "^3.0.37",
    "@types/jasmine": "^2.5.43",
    "@types/mkdirp": "^0.3.29",
    "@types/node": "^7.0.5",
    "@types/rimraf": "0.0.28",
